using System;

class Program
{
    static void Main()
    {
        int[,] tablica = new int[10, 10];
        int liczba = 1;
        int wiersze = 10;
        int kolumny = 10;
        int dol = wiersze - 1;
        int prawo = kolumny - 1;
        int gora = 0;
        int lewo = 0;

        while (liczba <= wiersze * kolumny)
        {
            // Wypełnianie wiersza od lewej do prawej
            for (int j = lewo; j <= prawo; j++)
            {
                tablica[gora, j] = liczba;
                liczba++;
            }
            gora++;

            // Wypełnianie kolumny od góry do dołu
            for (int i = gora; i <= dol; i++)
            {
                tablica[i, prawo] = liczba;
                liczba++;
            }
            prawo--;

            // Wypełnianie wiersza od prawej do lewej
            for (int j = prawo; j >= lewo; j--)
            {
                tablica[dol, j] = liczba;
                liczba++;
            }
            dol--;

            // Wypełnianie kolumny od dołu do góry
            for (int i = dol; i >= gora; i--)
            {
                tablica[i, lewo] = liczba;
                liczba++;
            }
            lewo++;
        }

        Console.WriteLine("Wypełniona tablica:");
        for (int i = 0; i < wiersze; i++)
        {
            for (int j = 0; j < kolumny; j++)
            {
                Console.Write(tablica[i, j] + " ");
            }
            Console.WriteLine();
        }
    }
}
